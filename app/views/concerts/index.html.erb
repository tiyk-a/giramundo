<p id="notice"><%= notice %></p>

<h1>Concerts</h1>

<table>
  <thead>
    <tr>
      <th>Concert name</th>
      <th>Source</th>
      <th>Image</th>
      <th>Tm</th>
      <th>Date</th>
      <th>Timezone</th>
      <th>Local date</th>
      <th>Venue</th>
      <th>Origin</th>
      <th>Deleted at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @concerts.each do |concert| %>
      <tr>
        <td><%= concert.concert_name %></td>
        <td><%= concert.source %></td>
        <td><%= concert.image %></td>
        <td><%= concert.tm_id %></td>
        <td><%= concert.date %></td>
        <td><%= concert.timezone %></td>
        <td><%= concert.local_date %></td>
        <td><%= concert.venue_id %></td>
        <td><%= concert.origin %></td>
        <td><%= concert.deleted_at %></td>
        <td><%= link_to 'Show', concert %></td>
        <td><%= link_to 'Edit', edit_concert_path(concert) %></td>
        <td><%= link_to 'Destroy', concert, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Concert', new_concert_path %>
